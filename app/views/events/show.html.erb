<br /> Creator is <%= @event.creator.name %> <br />

<h3> Attendee Names </h3>
<ul>
    <% @event.attendees.each do |attendee| %>
    <li>
        <%= attendee.name %>
    </li>
    <% end %>
</ul>
<%= form_for AttendeeEvent.create do |f|%>
    <%= f.label :attendee_id %>
    <%= f.text_field :attendee_id %>
    <%= f.hidden_field :event_id, value: @event.id %>
    <%= f.submit "Submit" %>
<% end %>

<%= link_to "All events ", events_path %>